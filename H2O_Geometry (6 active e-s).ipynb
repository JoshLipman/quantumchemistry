   "source": [
    "Explanation of [qml.finite_diff](https://pennylane.readthedocs.io/en/stable/code/api/pennylane.finite_diff.html#pennylane.finite_diff)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {
    "id": "ke8e5uK5oa96"
   },
   "outputs": [],
   "source": [
    "# Find the partial differential of H w.r.t. x\n",
    "\n",
    "def grad_x(params, x):\n",
    "    grad_h = qml.finite_diff(H)(x)\n",
    "    grad = [circuit(params, obs=obs, wires=range(num_wires)) for obs in grad_h]\n",
    "    return np.array(grad)"
   ]
  },
